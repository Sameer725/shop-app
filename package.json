{
  "name": "kleinstark",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall": "node utils/preinstall-script.js",
    "postinstall": "patch-package",
    "generate-types": "graphql-codegen --config ./codegen.json",
    "prepare": "husky install",
    "android": "react-native run-android --variant=DevDebug --appIdSuffix 'dev'",
    "ios": "react-native run-ios --scheme 'Kleinstark.Local' --simulator='iPhone 8'",
    "android-and-ios": "yarn run android && yarn run ios",
    "start": "react-native start",
    "test": "jest",
    "icons": "ts-node utils/generate-icon-font.ts && npx react-native link",
    "lint": "eslint '**/*.{ts,tsx}'",
    "lint-fix": "eslint '**/*.{ts,tsx}' --fix",
    "prettier-base": "prettier --config .prettierrc.json --ignore-path .prettierignore '**/*.{js,jsx,ts,tsx}'",
    "prettier": "yarn run prettier-base -- --check",
    "prettier-fix": "yarn run prettier-base -- --write"
  },
